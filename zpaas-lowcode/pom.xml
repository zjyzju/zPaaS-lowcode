	<modelVersion>4.0.0</modelVersion>
	
	<groupId>cn.zpaas.lowcode</groupId>
	<artifactId>zpaas-lowcode</artifactId>
	<version>0.0.1-SNAPSHOT</version>
	<name>zpaas-lowcode</name>
	<packaging>pom</packaging>

	<modules>
		<module>zpaas-lowcode-common</module>
		<module>zpaas-lowcode-domain</module>
		<module>zpaas-lowcode-be-engine</module>
		<module>zpaas-lowcode-be-engine-ext</module>
		<module>zpaas-lowcode-be-ide</module>
		<module>zpaas-lowcode-be-ide-ext</module>
		<module>zpaas-lowcode-fe-engine</module>
