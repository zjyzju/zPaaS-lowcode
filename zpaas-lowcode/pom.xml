		<module>zpaas-lowcode-bi-engine</module>
		<module>zpaas-lowcode-bi-ide</module>
		<module>zpaas-lowcode-starter</module>
	</modules>

	<properties>
		<java.version>21</java.version>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		<maven.compiler.source>21</maven.compiler.source>
		<maven.compiler.target>21</maven.compiler.target>
		<spring-boot.version>3.2.4</spring-boot.version>
		<spring.version>6.1.15</spring.version>
		<slf4j.version>2.0.12</slf4j.version>
		<logback.version>1.5.3</logback.version>
		<gson.version>2.10.1</gson.version>
		<ibatis.version>3.0</ibatis.version>
		<mybatis-generator.version>1.4.0</mybatis-generator.version>
		<mysql-connector.version>8.4.0</mysql-connector.version>
		<guava.version>33.1.0-jre</guava.version>
		<mybatis.version>3.0.3</mybatis.version>
		<poi.version>5.4.1</poi.version>
		<poi-schemas.version>4.1.2</poi-schemas.version>
		<mail-api.version>2.1.3</mail-api.version>
		<jakarta-mail.version>2.0.3</jakarta-mail.version>
		<activation-api.version>2.1.3</activation-api.version>
		<camunda.version>7.20.0</camunda.version>
		<jedis.version>5.1.2</jedis.version>
		<hessian.version>4.0.66</hessian.version>
		<minio.version>8.5.9</minio.version>
		<caffeine.version>3.1.8</caffeine.version>
		<kafka-clients.version>3.8.1</kafka-clients.version>
		<jsch.version>0.2.17</jsch.version>
		<commons-net.version>3.10.0</commons-net.version>
		<jasypt.version>3.0.5</jasypt.version>
		<jakarta.servlet.version>6.0.0</jakarta.servlet.version>
		<jakarta.annotation.version>3.0.0</jakarta.annotation.version>
		<quartz.version>2.5.0</quartz.version>
		<xxljob.version>2.4.2</xxljob.version>
		<bcpkix.version>1.78.1</bcpkix.version>
		<json-path.version>2.9.0</json-path.version>
		<gson.version>2.10.1</gson.version>
	</properties>

	<dependencyManagement>
		<dependencies>
			<dependency>
				<groupId>com.github.ulisesbocchio</groupId>
				<artifactId>jasypt-spring-boot-starter</artifactId>
